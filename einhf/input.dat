
# PYTHONPATH must include directory above plugin directory.
#     Define either externally or here, then import plugin.
sys.path.insert(0, './..')
import einhf

set reference rhf

molecule {
O
H 1 R
H 1 R 2 A

R = .9
A = 104.5

}

set {
  basis cc-pVTZ
}

set GUESS CORE
set DIIS FALSE
set SCF_INITIAL_ACCELERATOR NONE
set PRINT 1
set SCF_TYPE PK
set COMPUTE GPU

E1 = energy("hf")

E2 = energy('einhf')

assert(abs(E2 - E1) < 1e-6)

