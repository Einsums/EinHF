
# PYTHONPATH must include directory above plugin directory.
#     Define either externally or here, then import plugin.
sys.path.insert(0, './..')
import einhf

set reference uhf

molecule {
O
H 1 R
H 1 R 2 A

R = .9
A = 104.5

}

set {
  basis sto-3g
}

set GUESS CORE
set DIIS TRUE
set SCF_INITIAL_ACCELERATOR NONE
set PRINT 1
set SCF_TYPE PK
set COMPUTE CPU

E1 = energy('einhf')

E2 = energy('scf')

assert(abs(E1 - E2) < 1e-8)


einhf.exampleFN()
