
# PYTHONPATH must include directory above plugin directory.
#     Define either externally or here, then import plugin.
sys.path.insert(0, './..')
import einks

molecule {
O
H 1 R
H 1 R 2 A

R = .9
A = 104.5
}

set {
  basis sto-3g
}

set einks {
  print 1
}

E1 = energy('einks')

E2 = energy('b3lyp')

assert(abs(E1 - E2) < 1e-8)

einks.exampleFN()
